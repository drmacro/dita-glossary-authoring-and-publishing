            <dt class="- topic/dt " xtrc="dt:1;17:37" xtrf="file:/Users/waybri01/gitrepos/dita-glossary-authoring-and-publishing/w_glossary/SampleBook/arm1/dav1330616213038.xml">Signal level</dt>
            <dd class="- topic/dd " xtrc="dd:1;18:37" xtrf="file:/Users/waybri01/gitrepos/dita-glossary-authoring-and-publishing/w_glossary/SampleBook/arm1/dav1330616213038.xml">
